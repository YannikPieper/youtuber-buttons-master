<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="2dip" />
            <stroke android:width="3dip" android:color="#F65200" />
            <gradient android:angle="-90" android:startColor="#5D5D5D" android:endColor="#787878"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="2dip" />
            <stroke android:width="3dip" android:color="#F65200" />
            <solid android:src="@drawable/eticon"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="2dip" />
            <stroke android:width="3dip" android:color="#F65200" />
            <gradient android:angle="-90" android:startColor="#787878" android:endColor="#5D5D5D" />
        </shape>
    </item>
</selector>